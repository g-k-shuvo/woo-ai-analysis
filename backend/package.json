{
  "name": "woo-ai-analytics-backend",
  "version": "1.0.0",
  "description": "SaaS backend for Woo AI Analytics",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/ tests/ --ext .ts",
    "lint:fix": "eslint src/ tests/ --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration",
    "migrate": "knex --knexfile db/knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile db/knexfile.ts migrate:rollback",
    "seed": "knex --knexfile db/knexfile.ts seed:run"
  },
  "dependencies": {
    "fastify": "^5.2.1",
    "knex": "^3.1.0",
    "pg": "^8.13.1",
    "ioredis": "^5.4.2",
    "bullmq": "^5.34.8",
    "pino": "^9.6.0",
    "openai": "^4.78.1",
    "dotenv": "^16.4.7",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "tsx": "^4.19.2",
    "@types/node": "^22.12.0",
    "@types/pg": "^8.11.11",
    "@types/bcrypt": "^5.0.2",
    "eslint": "^8.57.1",
    "@typescript-eslint/parser": "^7.18.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "prettier": "^3.4.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "@types/jest": "^29.5.14"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
